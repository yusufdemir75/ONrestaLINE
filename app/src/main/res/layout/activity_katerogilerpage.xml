<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    android:background="@drawable/ahsaparkaplan"
    tools:context=".Katerogilerpage">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="1dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:id="@+id/textView5"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:layout_margin="10dp"
                android:fontFamily="@font/sonsie_one"
                android:gravity="center"
                android:text="ONrestaLİNE MENÜ"
                android:textColor="#000000"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/textView7"
                android:layout_width="300dp"
                android:layout_height="35dp"
                android:gravity="center"
                android:text="ÇORBA"
                android:textColor="#000000"
                android:textSize="24sp" />

            <ImageView
                android:id="@+id/corbaview"
                android:layout_width="300dp"
                android:layout_height="200dp"
                android:layout_gravity="start"
                app:srcCompat="@drawable/corba" />

            <TextView
                android:id="@+id/textView8"
                android:layout_width="300dp"
                android:layout_height="35dp"
                android:layout_gravity="end"
                android:gravity="center_horizontal"
                android:text="ANA YEMEK"
                android:textColor="#000000"
                android:textSize="24sp" />

            <ImageView
                android:id="@+id/anayemekview"
                android:layout_width="300dp"
                android:layout_height="200dp"
                android:layout_gravity="end"

                app:srcCompat="@drawable/anayemek" />

            <TextView
                android:id="@+id/textView9"
                android:layout_width="300dp"
                android:layout_height="35dp"
                android:gravity="center"
                android:text="SALATA"
                android:textColor="#000000"
                android:textSize="24sp" />

            <ImageView
                android:id="@+id/salataview"
                android:layout_width="300dp"
                android:layout_height="200dp"
                app:srcCompat="@drawable/salata" />

            <TextView
                android:id="@+id/textView6"
                android:layout_width="300dp"
                android:layout_height="35dp"
                android:layout_gravity="end"
                android:gravity="center"
                android:text="TATLI"

                android:textColor="#000000"
                android:textSize="24sp" />

            <ImageView
                android:id="@+id/tatliview"
                android:layout_width="300dp"
                android:layout_height="200dp"
                android:layout_gravity="end"

                app:srcCompat="@drawable/tatli" />

            <TextView
                android:id="@+id/textView11"
                android:layout_width="300dp"
                android:layout_height="35dp"
                android:gravity="center"
                android:text="SOĞUK İÇECEKLER"
                android:textColor="#000000"
                android:textSize="24sp" />

            <ImageView
                android:id="@+id/sogukicecekview"
                android:layout_width="300dp"
                android:layout_height="200dp"
                app:srcCompat="@drawable/icecek" />

            <TextView
                android:id="@+id/textView12"
                android:layout_width="300dp"
                android:layout_height="35dp"
                android:layout_gravity="end"
                android:gravity="center"
                android:text="SICAK İÇECEKLER"

                android:textColor="#000000"
                android:textSize="24sp" />

            <ImageView
                android:id="@+id/sicakicicekview"
                android:layout_width="300dp"
                android:layout_height="200dp"
                android:layout_gravity="end"

                app:srcCompat="@drawable/sicakicecek" />
        </LinearLayout>
    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>